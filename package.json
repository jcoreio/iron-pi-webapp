    "jss-nested": "^6.0.1",
    "mindfront-redux-utils": "^1.2.3",
    "mindfront-redux-utils-immutable": "^1.3.0",
    "mysql2": "^1.5.1",
    "p-timeout": "^2.0.1",
    "promisify-event": "^1.0.0",
    "prop-types": "^15.5.8",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-jss": "^8.1.0",
    "react-redux": "^5.0.2",
    "react-redux-features": "^2.0.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-drilldown": "^2.0.4",
    "react-transition-context": "^2.0.1",
    "react-view-slider": "^3.0.1",
    "redis": "^2.6.5",
    "redux": "^3.5.2",
    "redux-batched-actions": "^0.1.5",
    "redux-features": "^3.0.0",
    "redux-form": "^7.2.0",
    "redux-track-promise": "^2.1.0",
    "sequelize": "^4.23.2",
    "superagent": "^3.6.0",
    "superagent-logger": "^1.1.0",
    "superagent-use": "^0.1.0",
    "umzug": "^2.1.0",
    "url-parse": "^1.1.7"
  },
  "version": "1.0.0",
